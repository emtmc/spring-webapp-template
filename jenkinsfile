pipeline{
    agent any
    tools {
        maven 'my_maven_3.6.0'
    }
    stages{
    stage('Build') {
        steps {
            sh 'mvn clean compile'
        }
    }
    stage('Testing') {
        steps{
            sh 'mvn test verify'
        }
    }
    stage('Package') {
        steps {
            sh 'mvn package'
        }
    }
    stage('Deploy'){
        steps{

    sshagent (credentials: ['tomcat-dev']) {
          sh 'scp var/lib/jenkins/workspace/Template/target/*.war '
}
        }
    }
    }
}