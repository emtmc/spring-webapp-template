pipeline{
    agent any
    tools {
        maven 'my_maven_3.6.0'
    }
    stages{
    stage('Build') {
        steps {
            sh 'mvn clean compile'
        }
    }
    stage('Testing') {
        steps{
            sh 'mvn test verify'
        }
    }
    stage('Package') {
        steps {
            sh 'mvn package'
        }
    }
    stage('Deploy'){
        steps{
            sshagent (credentials: ['tomcat-dev']) {
                sh 'scp target/*.war jenkinsAdmin@13.82.82.176:9999/tomcat/apache-tomcat-8.5.38/webapps'
            }
        }
    }
    }
}